<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="sellingStation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="$shared/xml/schema/placeable.xsd">
    <storeData>
        <name>$l10n_animals_dealer</name>
        <image>maps/placeables/sellingStations/store_sellingStationAD.png</image>
        <price>1</price>
        <canBeSold>false</canBeSold>
        <showInStore>false</showInStore>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
        <brand>NONE</brand>
        <species>placeable</species>
        <category>placeableMisc</category>
        <vertexBufferMemoryUsage>367104</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>36096</indexBufferMemoryUsage>
        <textureMemoryUsage>2097152</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
    </storeData>

    <base>
        <filename>maps/placeables/sellingStations/sellingStationAD.i3d</filename>
    </base>

    <placement useRandomYRotation="false" useManualYRotation="true" />

    <clearAreas></clearAreas>

    <leveling></leveling>

    <indoorAreas></indoorAreas>

    <hotspots>
        <hotspot type="UNLOADING" linkNode="0>7" teleportNode="0>8" />
    </hotspots>
	
    <triggerMarkers>
        <triggerMarker node="0>7" filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="true" />
    </triggerMarkers>

    <ai>
        <updateAreas>
            <updateArea startNode="0>1|0" endNode="0>1|0|0" />
        </updateAreas>
    </ai>

    <sellingStation supportsExtension="false" litersForFullPriceDrop="200000" fullPriceRecoverHours="48">
        <unloadTrigger exactFillRootNode="0>4"  fillTypes="WHEAT BARLEY SUNFLOWER MAIZE OAT SORGHUM POTATO LETTUCE TOMATO STRAW DRYGRASS_WINDROW GRASS_WINDROW SILAGE SUGARBEET_CUT DRYALFALFA_WINDROW ALFALFA_WINDROW CARROT">
            <baleTrigger triggerNode="0>5" deleteLitersPerSecond="10000"/>
        </unloadTrigger>
        <fillType name="WHEAT"            	priceScale="1.36" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="BARLEY"           	priceScale="1.3" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="SUNFLOWER"        	priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="MAIZE"            	priceScale="1.51" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="OAT"              	priceScale="1.3" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="SORGHUM"          	priceScale="1.33" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="POTATO"            	priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="LETTUCE"           	priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="TOMATO"            	priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="STRAW"             	priceScale="1.1" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="DRYGRASS_WINDROW"  	priceScale="1.2" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="GRASS_WINDROW"     	priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="SILAGE"            	priceScale="1.4" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="SUGARBEET_CUT"    	priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="DRYALFALFA_WINDROW" priceScale="1.9" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="ALFALFA_WINDROW"	priceScale="1.9" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="CARROT"				priceScale="1.8" supportsGreatDemand="true" disablePriceDrop="false" />
    </sellingStation>

</placeable>
