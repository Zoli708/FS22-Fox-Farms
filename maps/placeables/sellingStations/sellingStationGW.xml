<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="sellingStation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="$shared/xml/schema/placeable.xsd">
    <storeData>
        <name>$l10n_station_us_grainSiloRailroad01</name>
        <image>maps/placeables/sellingStations/store_sellingStationGW.png</image>
        <price>1</price>
        <canBeSold>false</canBeSold>
        <showInStore>false</showInStore>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
        <brand>NONE</brand>
        <species>placeable</species>
        <category>placeableMisc</category>
        <vertexBufferMemoryUsage>367104</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>36096</indexBufferMemoryUsage>
        <textureMemoryUsage>2097152</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
    </storeData>

    <base>
        <filename>maps/placeables/sellingStations/sellingStationGW.i3d</filename>
    </base>

    <placement useRandomYRotation="false" useManualYRotation="true" />

    <clearAreas></clearAreas>

    <leveling></leveling>

    <indoorAreas></indoorAreas>

    <hotspots>
        <hotspot type="UNLOADING" linkNode="0>7" teleportNode="0>8" />
    </hotspots>
	
    <triggerMarkers>
        <triggerMarker node="0>7" filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="false" />
    </triggerMarkers>

    <ai>
        <updateAreas>
            <updateArea startNode="0>10" endNode="0>10|0" />
        </updateAreas>
    </ai>

    <sellingStation supportsExtension="false" litersForFullPriceDrop="200000" fullPriceRecoverHours="48">
        <unloadTrigger exactFillRootNode="0>4"  fillTypes="WHEAT BARLEY CANOLA SUNFLOWER SOYBEAN MAIZE OAT SORGHUM"/>
        <fillType name="WHEAT"            priceScale="1.2" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="BARLEY"           priceScale="1.2" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="CANOLA"           priceScale="1.7" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="SUNFLOWER"        priceScale="1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="SOYBEAN"          priceScale="1.35" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="MAIZE"            priceScale="1.2" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="OAT"              priceScale="1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="SORGHUM"          priceScale="1.13" supportsGreatDemand="true" disablePriceDrop="false" />

    </sellingStation>

</placeable>
