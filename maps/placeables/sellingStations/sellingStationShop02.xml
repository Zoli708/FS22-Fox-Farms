<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="sellingStation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="$shared/xml/schema/placeable.xsd">
    <storeData>
        <name>
			<en>Grain mill warehouse</en>
			<de>Getreidemühlenlager</de>
			<fr>Entrepôt de moulin à grains</fr>
		</name>
        <image>maps/placeables/sellingStations/store_warehouse.png</image>
        <price>1</price>
        <canBeSold>false</canBeSold>
        <showInStore>false</showInStore>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
        <brand>NONE</brand>
        <species>placeable</species>
        <category>placeableMisc</category>
        <vertexBufferMemoryUsage>367104</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>36096</indexBufferMemoryUsage>
        <textureMemoryUsage>2097152</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
    </storeData>

    <base>
        <filename>maps/placeables/sellingStations/sellingStationShop02.i3d</filename>
    </base>

    <placement useRandomYRotation="false" useManualYRotation="true" />

    <clearAreas></clearAreas>

    <leveling></leveling>

    <indoorAreas></indoorAreas>

    <hotspots>
        <hotspot type="UNLOADING" linkNode="0>4" teleportNode="0>5" />
    </hotspots>
	
    <triggerMarkers>
        <triggerMarker node="0>4" filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="true" />
    </triggerMarkers>

    <ai>
        <updateAreas>
            <updateArea startNode="0>7" endNode="0>7|0" />
        </updateAreas>
    </ai>

    <sellingStation supportsExtension="false" litersForFullPriceDrop="200000" fullPriceRecoverHours="48">
        <unloadTrigger exactFillRootNode="0>1"  fillTypes="WHEAT BARLEY CANOLA SUNFLOWER SOYBEAN MAIZE OAT SORGHUM POTATO SUGARBEET SUGARCANE STRAWBERRY LETTUCE TOMATO GRAPE OLIVE FURNITURE CARROT"/>
        <fillType name="WHEAT"            priceScale="1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="BARLEY"           priceScale="1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="CANOLA"           priceScale="1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="SUNFLOWER"        priceScale="1.44" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="SOYBEAN"          priceScale="1.1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="MAIZE"            priceScale="1.4" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="OAT"              priceScale="1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="SORGHUM"          priceScale="1.32" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="POTATO"           priceScale="1.1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="SUGARBEET"        priceScale="0.8" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="SUGARCANE"        priceScale="1.1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="STRAWBERRY"       priceScale="2.21" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="LETTUCE"          priceScale="1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="TOMATO"           priceScale="1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="GRAPE"            priceScale="1.46" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="OLIVE"            priceScale="1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="FURNITURE"        priceScale="1.3" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="CARROT"        	  priceScale="1.7" 	supportsGreatDemand="true" disablePriceDrop="false" />
    </sellingStation>

</placeable>
