<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="sellingStation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="$shared/xml/schema/placeable.xsd">
    <storeData>
        <name>
			<en>Provisions storehouse</en>
			<de>Proviantlager</de>
			<fr>Magasin de provisions</fr>
		</name>
        <image>maps/placeables/sellingStations/store_warehouse.png</image>
        <price>1</price>
        <canBeSold>false</canBeSold>
        <showInStore>false</showInStore>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
        <brand>NONE</brand>
        <species>placeable</species>
        <category>placeableMisc</category>
        <vertexBufferMemoryUsage>367104</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>36096</indexBufferMemoryUsage>
        <textureMemoryUsage>2097152</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
    </storeData>

    <base>
        <filename>maps/placeables/sellingStations/sellingStationShop02.i3d</filename>
    </base>

    <placement useRandomYRotation="false" useManualYRotation="true" />

    <clearAreas></clearAreas>

    <leveling></leveling>

    <indoorAreas></indoorAreas>

    <hotspots>
        <hotspot type="UNLOADING" linkNode="0>4" teleportNode="0>5" />
    </hotspots>
	
    <triggerMarkers>
        <triggerMarker node="0>4" filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="true" />
    </triggerMarkers>

    <ai>
        <updateAreas>
            <updateArea startNode="0>7" endNode="0>7|0" />
        </updateAreas>
    </ai>

    <sellingStation supportsExtension="false" litersForFullPriceDrop="200000" fullPriceRecoverHours="48">
        <unloadTrigger exactFillRootNode="0>1"  fillTypes="WHEAT BARLEY OAT SORGHUM POTATO BREAD CAKE CHEESE GRAPEJUICE SUNFLOWER_OIL CHOCOLATE STRAWBERRY LETTUCE TOMATO FLOUR BUTTER SUGAR CEREAL CANOLA_OIL OLIVE_OIL GRAPE OLIVE RAISINS MILK EGG HONEY CARROT"/>
        <fillType name="WHEAT"            priceScale="0.8" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="BARLEY"           priceScale="0.8" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="OAT"              priceScale="0.8" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="SORGHUM"          priceScale="0.8" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="POTATO"           priceScale="1.1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="BREAD"            priceScale="1.2" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="CAKE"             priceScale="1.3" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="CHEESE"           priceScale="1.3" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="GRAPEJUICE"       priceScale="1.5" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="SUNFLOWER_OIL"    priceScale="1.1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="CHOCOLATE"        priceScale="1.8" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="STRAWBERRY"       priceScale="2.1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="LETTUCE"          priceScale="1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="TOMATO"           priceScale="1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="FLOUR"            priceScale="1.4" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="BUTTER"           priceScale="1.6" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="SUGAR"            priceScale="1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="CEREAL"           priceScale="1.6" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="CANOLA_OIL"       priceScale="1.1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="OLIVE_OIL"        priceScale="1.1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="GRAPE"            priceScale="1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="OLIVE"            priceScale="1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="RAISINS"          priceScale="1.2" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="MILK"   	      priceScale="1.2" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="EGG"              priceScale="1" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="HONEY"            priceScale="1.8" 	supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="CARROT"           priceScale="1.75" supportsGreatDemand="true" disablePriceDrop="false" />
    </sellingStation>

</placeable>
