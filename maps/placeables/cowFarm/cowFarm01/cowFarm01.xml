<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="cowHusbandry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="$shared/xml/schema/placeable.xsd">
    <storeData>
        <name>$l10n_storeItem_animalBarnCowBig</name>
        <image>maps/placeables/cowFarm/store_cow.png</image>
        <price>97050</price>
        <functions>
            <function>$l10n_function_animalPenCow</function>
        </functions>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
        <brand>none</brand>
        <species>placeable</species>
        <category>animalpens</category>
        <brush>
            <type>placeable</type>
            <category>animals</category>
            <tab>cows</tab>
        </brush>
		
		<canBeSold>true</canBeSold>
        <showInStore>false</showInStore>
		<!-- <boughtWithFarmland>true</boughtWithFarmland> -->
		
        <vertexBufferMemoryUsage>5529088</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>998912</indexBufferMemoryUsage>
        <textureMemoryUsage>10289152</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>

        <storeIconRendering>
            <settings cameraXRot="-20" cameraYRot="50" advancedBoundingBox="true"/>
        </storeIconRendering>
        <audioMemoryUsage>0</audioMemoryUsage>
    </storeData>

    <base>
        <filename>maps/placeables/cowFarm/cowFarm01/cowFarm01.i3d</filename>
        <canBeRenamed>true</canBeRenamed>
		<boughtWithFarmland>true</boughtWithFarmland>
    </base>

    <placement useRandomYRotation="false" useManualYRotation="true" >
    </placement>

    <clearAreas>
    </clearAreas>

    <leveling >
    </leveling>

    <foliageAreas>
    </foliageAreas>

    <indoorAreas>
    </indoorAreas>

    <triggerMarkers>
        <triggerMarker node="0>8|0"         filename="$data/shared/assets/marker/markerIcons.xml" id="LOAD" />
        <triggerMarker node="0>8|3"         filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" />
        <triggerMarker node="0>8|1"      	filename="$data/shared/assets/marker/markerIcons.xml" id="ANIMALS" />
        <triggerMarker node="0>8|2" 		filename="$data/shared/assets/marker/markerIcons.xml" id="LOAD" />
    </triggerMarkers>

    <tipOcclusionUpdateAreas>
    </tipOcclusionUpdateAreas>

    <ai>
        <updateAreas>
            <updateArea startNode="0>11|0" endNode="0>11|0|0" />
        </updateAreas>
    </ai>

    <hotspots>
        <hotspot type="COW" teleportNode="0>0" />
    </hotspots>

    <infoTrigger triggerNode="0>9" />

    <husbandry saveId="Animals_COW" hasStatistics="false">
        <storage node="2" fillTypes="LIQUIDMANURE MILK STRAW MANURE WATER" isExtension="false">
            <capacity fillType="LIQUIDMANURE" capacity="135000" />
            <capacity fillType="MILK" capacity="60000" />
            <capacity fillType="STRAW" capacity="38000" />
            <capacity fillType="MANURE" capacity="0" />
			<capacity fillType="WATER" capacity="31000" />
        </storage>

        <unloadingStation supportsExtension="true" storageRadius="65" hideFromPricesMenu="true">
            <unloadTrigger exactFillRootNode="0>3|1" fillTypes="STRAW" />
            <unloadTrigger exactFillRootNode="0>6|0" fillTypes="WATER" /> <!-- aiNode="0>6|2" -->
            <unloadTrigger fillTypes="MANURE MILK LIQUIDMANURE" />
        </unloadingStation>

        <loadingStation supportsExtension="true" storageRadius="50" fillTypes="WATER">
            <loadTrigger triggerNode="0>5|0" fillTypes="LIQUIDMANURE" fillLitersPerSecond="2000"  /> <!-- aiNode="0>5|1" -->
            <loadTrigger triggerNode="0>4|0" fillTypes="MILK" fillLitersPerSecond="2000" /> <!-- aiNode="0>4|1" -->
        </loadingStation>

        <animals type="COW" maxNumAnimals="200" >
            <navigation rootNode="0>7" filename="maps/placeables/cowFarm/cowFarm01/navMesh.i3d" />
            <loadingTrigger node="0>10" />
        </animals>

        <straw>
            <manure factor="1.5" />
            <strawPlane node="0>3|0" minY="-0.2" maxY="0.0" colorChange="false" />
        </straw>

        <water automaticWaterSupply="false" >
            <waterPlane node="0>6|3" minY="0.025" maxY="0.177" />
            <waterPlaces>
                <waterPlace node="0>6|1|0" />
                <waterPlace node="0>6|1|1" />
				<waterPlace node="0>6|1|2" />
                <waterPlace node="0>6|1|3" />
                <waterPlace node="0>6|1|4" />
                <waterPlace node="0>6|1|5" />
            </waterPlaces>
        </water>

        <liquidManure />

        <food capacity="80000">
            <foodPlaces>
                <foodPlace node="0>1|3|0" />
                <foodPlace node="0>1|3|1" />
                <foodPlace node="0>1|3|2" />
                <foodPlace node="0>1|3|3" />
                <foodPlace node="0>1|3|4" />
                <foodPlace node="0>1|3|5" />
                <foodPlace node="0>1|3|6" />
            </foodPlaces>
            <dynamicFoodPlane node="0>1|0" defaultFillType="forage"/>
            <feedingTrough exactFillRootNode="0>1|1">
                <baleTrigger triggerNode="0>1|2"/>
            </feedingTrough>
        </food>

		<fences>
        </fences>
    </husbandry>

    <!-- <deletedNodes> -->
        <!-- <deletedNode node="tipCollisionVector" /> -->
        <!-- <deletedNode node="infoTriggerVector" /> -->
    <!-- </deletedNodes> -->

</placeable>
